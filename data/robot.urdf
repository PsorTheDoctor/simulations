			</inertial>
	</link>

<joint name="joint_P_1" type="revolute">
    <parent link="podstawa"/>
    <child link="link_1"/>
    <axis xyz="0 0 1"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <limit effort="1000.0" lower="-1.48" upper="1.48" velocity="1.0"/>
  </joint>

  <joint name="joint_1_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
