<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro" name="moj_robot">

	<xacro:macro name="cylinder_inercja" params="m R L">
    <inertia ixx="${0.0833333*m*(3*R*R+L*L)}" ixy="0" ixz="0" iyy="${0.0833333*m*(3*R*R+L*L)}" iyz="0" izz="${0.5*m*R*R}"/>
  </xacro:macro>

	<link name="podstawa">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.1" radius="0.1"/>
			</geometry>
			<material name="blue">
				<color rgba="0.25 0.6 0.9 1"/>
			</material>
		</visual>

		<collision>
			<geometry>
				<cylinder length="0.1" radius="0.1"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 0 0"/>
		</collision>
		   <inertial>
				<origin rpy="0 0 0" xyz="0 0 0.05"/>
				<mass value="1"/>
				<xacro:cylinder_inercja m="1" R="0.1" L="0.1"/>
			</inertial>
	</link>

	<link name="link_1">
		<visual>
			<origin xyz="0 0 0.1" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.1" radius="0.1"/>
