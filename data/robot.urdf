				<cylinder length="0.1" radius="0.1"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 0 0.1"/>
		</collision>
		   <inertial>
				<origin rpy="0 0 0" xyz="0 0.0 0.1"/>
				<mass value="1"/>
				<xacro:cylinder_inercja m="1" R="0.1" L="0.1"/>
			</inertial>
	</link>


	<link name="link_2">
		<visual>
			<origin xyz="0 0.2 0.5" rpy="0 0 0"/>
			<geometry>
